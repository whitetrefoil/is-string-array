{
  "name": "@whitetrefoil/is-string-array",
  "version": "1.0.2",
  "description": "Is a string[]?",
  "main": "lib/common/main.js",
  "module": "lib/esm/main.js",
  "typings": "lib/types/main.d.ts",
  "scripts": {
    "prepublishOnly": "node scripts/prepublish-checklist.js && npm run build",
    "build": "rimraf lib && tsc && tsc -p tsconfig.common.json",
    "publish": "echo \"Done publishing, don't forget to:\n  1. **dist-tag** your channel in npm.\n  2. bump version in package.json\nGood luck with the publish ;)\"",
    "test": "tslint -p . && jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitetrefoil/is-string-array.git"
  },
  "keywords": [
    "array",
    "is",
    "string"
  ],
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "CC0-1.0",
  "files": [
    "lib/**/*.js",
    "lib/types/**/*.d.ts",
    "README.md"
  ],
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/whitetrefoil/is-string-array/issues"
  },
  "homepage": "https://github.com/whitetrefoil/is-string-array#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@whitetrefoil/check-dependencies": "^0.1.2",
    "jest": "^23.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {},
    "setupFiles": [],
    "testRegex": "(/__tests__/.*|(\\.|/)spec)\\.(js|ts)$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
